package sesliHarfler;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char harf='A';
		
		switch (harf) {
		case 'A':
		case 'I':
		case 'o':
		case 'U':
			System.out.println("kalin sesli harf");
			break;
		default:
			System.out.println("ince sesli harf");

		}
	}
}
